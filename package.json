{
  "name": "event-calendar",
  "version": "1.0.0",
  "description": "A simple calendar app with event management",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "backend": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "install:backend": "cd backend && npm install",
    "start:all": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "setup": "npm install && npm run install:backend"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "vite": "^5.0.11"
  }
}

